{
	"name": "photobooth",
	"version": "1.0.0",
	"description": "PhotoBooth",
	"port": 8888,
	"debug": 8889,
	"build": {
		"css": [
			"css.less",
			"css-photos.less",
			"css-print.less",
			"css-ie9.less"
		],
		"js": [
			"javascript.jss",
			"javascript-photos.jss",
			"javascript-ie8.jss",
			"javascript-ie9.jss"
		],
		"directories": [
			"/images",
			"/api"
		],
		"ignore": [
			"/node_modules/**/**",
			"node_modules/**/**",
			"/**/*.log",
			"log/*.log",
			"/**/*-*-*-*-*.jpg"
		],
		"pages": [
			"index.html",
			"booth.html"
		],
		"files": [
			"/favicon.ico"
		]
	}
}
